{
  "name": "demo-react-native",
  "version": "0.0.0",
  "description": "A demo to test React Native native modules, generated from: https://github.com/demchenkoalex/react-native-module-template#readme",
  "homepage": "https://github.com/demchenkoalex/react-native-module-template#readme",
  "author": "Ammar Ahmed",
  "license": "MIT",
  "files": [
    "android",
    "ios",
    "lib",
    "react-native-module-template.podspec",
    "!android/build",
    "!.DS_Store",
    "!.gradle",
    "!.idea",
    "!build",
    "!gradle",
    "!*.iml",
    "!gradlew",
    "!gradlew.bat",
    "!local.properties",
    "!project.xcworkspace",
    "!xcshareddata",
    "!xcuserdata"
  ],
  "scripts": {
    "compile": "rm -rf lib && tsc -p .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prepare": "yarn compile",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "file:../../node_modules/@types/react-native/@babel/core",
    "@babel/runtime": "file:../../node_modules/@types/react-native/@babel/runtime",
    "@react-native-community/eslint-config": "file:../../node_modules/@types/react-native/@react-native-community/eslint-config",
    "@types/jest": "file:../../node_modules/@types/react-native/@types/jest",
    "@types/react-native": "file:../../node_modules/@types/react-native",
    "@types/react-test-renderer": "file:../../node_modules/@types/react-test-renderer",
    "babel-jest": "file:../../node_modules/babel-jest",
    "eslint-plugin-simple-import-sort": "file:../../node_modules/eslint-plugin-simple-import-sort",
    "eslint": "file:../../node_modules/eslint",
    "jest": "file:../../node_modules/jest",
    "metro-react-native-babel-preset": "file:../../node_modules/metro-react-native-babel-preset",
    "react-native": "file:../../node_modules/react-native",
    "react-test-renderer": "file:../../node_modules/react-test-renderer",
    "react": "file:../../node_modules/react",
    "typescript": "file:../../node_modules/typescript"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
